container_commands:
  00_copy_env_file:
    command: "cp .env.example .env"
    cwd: "/var/app/staging"

  01_run_key_generate:
    command: "php artisan key:generate"
    cwd: "/var/app/staging"

  02_run_migration:
    command: "php artisan migrate:fresh"
    cwd: "/var/app/staging"
  
  03_run_seeding:
    command: "php artisan db:seed"
    cwd: "/var/app/staging"